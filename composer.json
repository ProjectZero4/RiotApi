{
    "name": "projectzero4/riot-api",
    "description": "A PHP library that allows you to easily use Riot Games API using Laravel Models and also stores the information in a database.",
    "type": "library",
    "authors": [
        {
            "name": "Josh Capener",
            "email": "josh@joshcapener.co.uk"
        }
    ],
    "require": {
        "php": "~8.1",
        "laravel/framework": "*",
        "predis/predis": "*",
        "laravel/horizon": "*",
        "spatie/laravel-data": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "ProjectZero4\\RiotApi\\": "src",
            "ProjectZero4\\RiotApi\\Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    }
}
